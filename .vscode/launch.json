    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${cwd}",
                "VERBOSITY": "DEBUG",
                "ENV_FILE": "./.env"
            }
        },
        {
            "name": "Python: WAP Telemetry Flow",
            "type": "debugpy",
            "request": "launch",
            "module": "case_study_telemetry.flows.wap_telemetry_flow",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${cwd}",
                "VERBOSITY": "DEBUG",
                "ENV_FILE": "./.env"
            }
        }
    ]
}
