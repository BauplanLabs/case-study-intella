# S3 Source Configuration
S3_SOURCE_BUCKET=intella-bauplan-data
S3_SOURCE_PATH=telemetry/raw/
S3_SOURCE_PATTERN=*.parquet

# Bauplan Data Configuration
BAUPLAN_NAMESPACE=telemetry
BAUPLAN_TARGET_TABLE=signals

# Prefect Configuration
PREFECT_API_URL=http://localhost:4200/api

# PostgreSQL (Prefect Backend)
POSTGRES_USER=prefect
POSTGRES_PASSWORD=prefect_password
POSTGRES_DB=prefect

# WAP Behavior
# on_success: "inspect" (show diff only) or "merge" (merge to main)
WAP_ON_SUCCESS=inspect
# on_failure: "keep" (keep branch for debugging) or "delete" (cleanup)
WAP_ON_FAILURE=keep

# AWS Credentials (required for S3 ingestion simulation)
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=eu-central-1
